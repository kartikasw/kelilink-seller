<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/se_tv_title"
        android:textStyle="bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/title_menu_empty"
        android:textAppearance="@style/TextAppearance.KelilinkSeller.Body1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/se_tv_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/content_menu_empty"
        android:gravity="center"
        android:layout_marginTop="@dimen/margin_component"
        android:layout_marginHorizontal="@dimen/margin_layout_more"
        android:textAppearance="@style/TextAppearance.KelilinkSeller.Body2"
        app:layout_constraintTop_toBottomOf="@+id/se_tv_title" />

</androidx.constraintlayout.widget.ConstraintLayout>